\f0\fs24 \cf0 Which are the top 10 members by spending\
\
SELECT\
    c.membership_id,\
    c.first_name,\
    c.last_name,\
    SUM(o.total_price) AS total_spent\
FROM\
    customers c\
JOIN orders o ON\
    c.membership_id = o.membership_id\
GROUP BY\
    c.membership_id,\
    c.first_name,\
    c.last_name\
ORDER BY\
    total_spent DESC\
LIMIT\
    10;\
\

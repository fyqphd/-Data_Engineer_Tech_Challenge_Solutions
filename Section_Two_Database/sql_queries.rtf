\cf0 Which are the top 3 items that are frequently brought by members\
\
SELECT\
    i.item_id,\
    i.item_name,\
    COUNT(o.order_id) AS times_purchased\
FROM\
    items i\
JOIN orders o ON\
    i.item_id = o.item_id\
GROUP BY\
    i.item_id,\
    i.item_name\
ORDER BY\
    times_purchased DESC\
LIMIT\
    3;\
